{
  "name": "paylink",
  "version": "0.0.1",
  "description": "The PayLink library provides convenient access to the PayLink API from applications written in JavaScript/Typescript",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/jest": "~27.5",
    "@types/luxon": "^3.0.0",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "jest": "~28.1",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "ts-node": "^10.9.1",
    "tsutils": "~3.21",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc && tsc --watch --preserveWatchOutput",
    "lint": "eslint . --ext .ts --ext .mts",
    "watch": "nodemon --ext ts,pug --ignore public",
    "prettier": "prettier --config .prettierrc --write ."
  },
  "author": "Hady 🍍",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "api": "^4.5.2",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "luxon": "^3.0.1",
    "nodemon": "^2.0.19",
    "tslib": "~2.4"
  }
}
